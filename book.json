{
  "title" : "Bota5ky's Blog",
  "author" : "Bota5ky",
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "mermaid-gb3",
    "chapter-fold",
    "search-pro",
    "code",
    "prism",
    "tbfed-pagefooter",
    "disqus",
    "-lunr",
    "-search",
    "-highlight",
    "-sharing"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ],
      "ignore": [
        "mermaid"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright Â©Bota5ky",
      "modify_label": "Last Updated: ",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "disqus": {
      "shortName": "bota5ky"
    }
  }
}